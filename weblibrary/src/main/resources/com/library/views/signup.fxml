<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.library.Controllers.SignupController"
      alignment="CENTER" spacing="20">
      
    <Label fx:id="title" />
    <TextField fx:id="username" />
    <TextField fx:id="password" />
    <TextField fx:id="email" />
    
    <TextField fx:id="ssnField" />
    <TextField fx:id="surname" />
    <TextField fx:id="firstname" />
    <Button fx:id="saveChangesButton" onAction="#saveChanges" />
    <Button fx:id="returnBackButton"  />
    
</VBox>